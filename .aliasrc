# my alias config
alias v='vim'

alias g='git'

alias ll='ls -alh'
alias la='ls -A'
alias l='ls -CF'

# add -v will stdout result
alias mkdir='mkdir -v'
alias mv='mv -v'
alias cp='cp -v'
alias rm='rm -vi' # -i 会有一个交互式的删除
alias ln='ln -v'

# tmux alias
alias tls='tmux ls'
alias tnew='tmux new -s'
alias tde='tmux detach'
alias tat='tmux attach -t'
alias tkill='tmux kill-session -t'

# python pip
alias python3.10="/opt/homebrew/opt/python@3.10/bin/python3"
alias pip="python -m pip"
# alias pip3.10="/opt/homebrew/opt/python@3.10/bin/python3 -m pip"
